<% @plots.each do |plot| %>
  <div id="plot-<%= plot.id %>">
    <h3><%= plot.number %></h3>
      <% plot.plants.each do |plant| %> 
        <p><%= plant.name %></p>
        <p><%= button_to "Remove", plot_plant_path(plot, plant), method: :delete, class: "remove-button" %></p>
      <% end %>
  </div>
<% end %>